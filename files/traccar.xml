<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>

    <entry key="config.default">./conf/default.xml</entry>

    <!-- For available parameters see https://www.traccar.org/configuration-file/ -->

    <entry key="database.driver">org.postgresql.Driver</entry>
    <entry key="database.password">###DATABASE_PASSWORD###</entry>
    <entry key="database.url">jdbc:###DATABASE_URL###</entry>
    <entry key="database.user">###DATABASE_USERNAME###</entry>
    <entry key="filter.accuracy">###FILTER_ACCURACY###</entry>
    <entry key="filter.enable">true</entry>
    <entry key="filter.skipLimit">###FILTER_SKIP_LIMIT###</entry>
    <entry key="logger.console">true</entry>
    <entry key="mail.smtp.auth">true</entry>
    <entry key="mail.smtp.from">###EMAIL_FROM###</entry>
    <entry key="mail.smtp.host">###EMAIL_HOST###</entry>
    <entry key="mail.smtp.password">###EMAIL_PASSWORD###</entry>
    <entry key="mail.smtp.port">###EMAIL_PORT###</entry>
    <entry key="mail.smtp.ssl.enable">true</entry>
    <entry key="mail.smtp.username">###EMAIL_USERNAME###</entry>
    <entry key="openid.adminGroup">###OIDC_ADMIN_GROUP###</entry>
    <entry key="openid.allowGroup">###OIDC_ALLOW_GROUP###</entry>
    <entry key="openid.clientId">###OIDC_CLIENT_ID###</entry>
    <entry key="openid.clientSecret">###OIDC_CLIENT_SECRET###</entry>
    <entry key="openid.force">###OIDC_FORCE###</entry>
    <entry key="openid.issuerUrl">###OIDC_ISSUER_URL###</entry>
    <entry key="web.port">80</entry>
    <entry key="web.url">###FRONTEND_URL###</entry>

</properties>
